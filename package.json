{
  "name": "teachers-agenda",
  "version": "1.0.0-alpha4",
  "private": true,
  "scripts": {
    "build": "run-p type-check \"build-only {@}\" --",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "build-only": "vite build",
    "dev": "concurrently npm:devL npm:firebase",
    "devL": "vite",
    "devE": "vite --host",
    "firebase": "firebase emulators:start --import emulator_data --export-on-exit emulator_data",
    "format": "prettier --write src/",
    "preview": "vite preview",
    "type-check": "vue-tsc --build --force"
  },
  "dependencies": {
    "@schedule-x/calendar-controls": "^2.12.1",
    "@schedule-x/drag-and-drop": "^2.7.0",
    "@schedule-x/event-modal": "^2.7.0",
    "@schedule-x/events-service": "^2.7.0",
    "@schedule-x/theme-default": "^2.5.0",
    "@schedule-x/vue": "^2.4.0",
    "@vee-validate/yup": "^4.13.2",
    "firebase": "^10.13.2",
    "pinia": "^2.1.7",
    "teachers-agenda": "file:",
    "uuid": "^10.0.0",
    "vee-validate": "^4.13.2",
    "vue": "^3.4.29",
    "vue-router": "^4.3.3",
    "vue3-toastify": "^0.2.3",
    "vuedraggable": "^4.1.0",
    "vuefire": "^3.2.0",
    "vuetify": "^3.7.1"
  },
  "devDependencies": {
    "@mdi/font": "^7.4.47",
